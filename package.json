{
  "name": "tisqleditor",
  "private": "true",
  "version": "0.0.1",
  "description": "a SQL editor based codemirror6",
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "release": "changeset publish"
  },
  "keywords": [
    "sql",
    "editor",
    "codemirror6"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.3.1"
  },
  "lint-staged": {
    "*.+(ts|tsx|js)": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.+(json|css|md|html)": "prettier --write"
  },
  "packageManager": "pnpm@9.3.0"
}
